{
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}",
    "Redis": "${REDIS_CONNECTION_STRING}"
  },
  
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System": "Warning"
    }
  },

  "Jwt": {
    "Key": "${JWT_SECRET_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpireMinutes": 60
  },

  "AllowedHosts": "*"
}
